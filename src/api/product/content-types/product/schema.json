{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid"
    },
    "name": {
      "type": "string",
      "required": true
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Electronics,",
        "Fashion,",
        "Home & Kitchen,",
        "Beauty & Personal Care,",
        "Sports & Outdoors,",
        "Books & Stationery,",
        "Health & Wellness,",
        "Toys & Games,",
        "Baby & Kids,",
        "Jewelry & Accessories"
      ],
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true,
      "default": 0
    },
    "description": {
      "type": "blocks",
      "required": true
    },
    "stock": {
      "type": "biginteger",
      "default": "0"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "rating": {
      "type": "decimal"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "products"
    }
  }
}
